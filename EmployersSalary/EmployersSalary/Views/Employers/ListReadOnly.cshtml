@model IEnumerable<EmployersSalary.Models.Employer>
@{
    ViewBag.Title = "ListReadOnly";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Employers</h2>

<table id="employers" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Gross Salary, €</th>
            <th>Net Salary, €</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>

@section scripts
{
    @Scripts.Render("~/bundles/ListReadOnly")  
}

